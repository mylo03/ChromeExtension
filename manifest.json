{
    "name": "URL Logger",
    "version": "1.0.3",
    "description": "Logs the date and URL of visited pages",
    "manifest_version": 3,
    "author": "Mylo Farman",
    
    "action": {
        "default_popup": "index.html",
        "default_title": "Journey To Hire"
    },

    "icons": {
        "128": "icon.png"
    }, 

    "content_security_policy": {
        "extension_pages": {
          "script-src": "'self';",
          "object-src": "'self'"
        }
    },

    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]

}