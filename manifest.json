{
    "name": "URL Logger",
    "version": "1.0.3",
    "description": "Logs the date and URL of visited pages",
    "manifest_version": 3,
    "author": "Mylo Farman",
    "action": {
        "default_popup": "index.html",
        "default_title": "Journey To Hire"
    },
    "icons": {
        "128": "icon.png"
    }, 

    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]

}